                            <pre><code class="language-javascript">import { EarnlyError, RateLimitError, AuthError } from '@earnly/sdk';

try {
  const recommendations = await earnly.query({
    user_query: "laptop recommendations"
  });
} catch (error) {
  if (error instanceof RateLimitError) {
    console.log('Rate limit exceeded, retry after:', error.retryAfter);
  } else if (error instanceof AuthError) {
    console.log('Authentication failed:', error.message);
  } else if (error instanceof EarnlyError) {
