import { ref, computed, onMounted } from 'vue'
import { useEarnly } from '@earnly/vue-sdk'

interface RecommendationContext {
  page: string
  category?: string
  tags?: string[]
  userPreferences?: Record&lt;string, any&gt;
}

interface Recommendation {
  id: string
  title: string
  description: string
  imageUrl: string
  price: string
  clickUrl: string
  metadata: Record&lt;string, any&gt;
}

const { trackEvent, getRecommendations, updateUserProfile } = useEarnly()

const context = ref&lt;RecommendationContext&gt;({
  page: 'product-detail',
  category: 'electronics',
  tags: ['smartphone', 'tech'],
  userPreferences: {
    priceRange: '$200-$800',
    brand: 'premium'
  }
})

const handleClick = (recommendation: Recommendation) =&gt; {
  trackEvent('recommendation_click', {
    recommendationId: recommendation.id,
    context: context.value,
    timestamp: Date.now()
  })
  
  // Navigate or perform action
  window.open(recommendation.clickUrl, '_blank')
}

const handleLoaded = (recommendations: Recommendation[]) =&gt; {
  trackEvent('recommendations_displayed', {
    count: recommendations.length,
    context: context.value
  })
}

const handleError = (error: Error) =&gt; {
  console.error('Recommendation error:', error)
  trackEvent('recommendation_error', {
    error: error.message,
    context: context.value
  })
}

const handleRecommendationClick = (recommendation: Recommendation) =&gt; {
  handleClick(recommendation)
}

const retryRecommendations = () =&gt; {
  // Component will automatically retry
  trackEvent('recommendation_retry', {
    context: context.value
  })
}

onMounted(() =&gt; {
  trackEvent('page_view', {
    page: 'recommendations',
    context: context.value
  })
})
