                        <h2 class="text-2xl font-bold text-gray-900 mb-4">HTTP Server Integration</h2>
                        
                        <div class="code-block mb-6">
                            <button class="copy-button" onclick="copyCode(this)">
                                <i class="fas fa-copy mr-1"></i>Copy
                            </button>
                            <pre class="bg-gray-900 text-gray-100 p-4 rounded-lg overflow-x-auto"><code class="language-go">package main

import (
    "context"
    "encoding/json"
    "fmt"
    "log"
    "net/http"
    "strconv"
    "time"

    "github.com/getearnly/earnly-go-sdk"
    "github.com/gorilla/mux"
)

type Server struct {
    client *earnly.Client
}

func NewServer(apiKey string) (*Server, error) {
    client, err := earnly.NewClient(&earnly.Config{
        APIKey:        apiKey,
        Environment:   earnly.Production,
        Timeout:       5 * time.Second,
        RetryAttempts: 3,
        EnableLogging: true,
    })
    if err != nil {
        return nil, err
    }

    return &Server{client: client}, nil
}

// RecommendationResponse represents the API response
type RecommendationResponse struct {
    Success         bool                      \`json:"success"\`
    Recommendations []earnly.Recommendation   \`json:"recommendations"\`
    Count           int                       \`json:"count"\`
    Error           string                    \`json:"error,omitempty"\`
}

// GetRecommendations handles GET /api/recommendations/{userID}
func (s *Server) GetRecommendations(w http.ResponseWriter, r *http.Request) {
    vars := mux.Vars(r)
    userID := vars["userID"]
    
    if userID == "" {
        http.Error(w, "User ID is required", http.StatusBadRequest)
        return
    }

    // Parse query parameters
    category := r.URL.Query().Get("category")
    page := r.URL.Query().Get("page")
    if page == "" {
        page = "homepage"
    }
    
    limitStr := r.URL.Query().Get("limit")
    limit := 6
    if limitStr != "" {
        if l, err := strconv.Atoi(limitStr); err == nil {
            limit = l
        }
    }

    // Build context from request
    context := map[string]interface{}{
        "page":      page,
        "userAgent": r.Header.Get("User-Agent"),
        "ipAddress": getClientIP(r),
        "referrer":  r.Header.Get("Referer"),
        "timestamp": time.Now().Unix(),
    }
    
    if category != "" {
        context["category"] = category
    }

    // Get recommendations
    recommendations, err := s.client.GetRecommendations(r.Context(), &earnly.RecommendationRequest{
        UserID:  userID,
        Context: context,
        Options: &earnly.RecommendationOptions{
            MaxRecommendations: limit,
            Personalized:      true,
            IncludeMetadata:   true,
            FilterSeen:        true,
        },
    })

    response := RecommendationResponse{
        Success: err == nil,
        Count:   len(recommendations),
    }

    if err != nil {
        response.Error = err.Error()
        w.WriteHeader(http.StatusInternalServerError)
    } else {
        response.Recommendations = recommendations
        
        // Track the recommendation request
        go s.trackEventAsync(r.Context(), &earnly.Event{
            Name:   "recommendations_requested",
            UserID: userID,
            Data: map[string]interface{}{
                "context": context,
                "count":   len(recommendations),
            },
        })
    }

    w.Header().Set("Content-Type", "application/json")
    json.NewEncoder(w).Encode(response)
}

// TrackEvent handles POST /api/track
func (s *Server) TrackEvent(w http.ResponseWriter, r *http.Request) {
    var eventRequest struct {
        Event  string                 \\`json:\"event\"\\`
        UserID string                 \\`json:\"userId\"\\`
        Data   map[string]interface{} \\`json:\"data\"\\`
    }

    if err := json.NewDecoder(r.Body).Decode(&eventRequest); err != nil {
        http.Error(w, "Invalid JSON", http.StatusBadRequest)
        return
    }

    if eventRequest.Event == "" || eventRequest.UserID == "" {
        http.Error(w, "Event and userID are required", http.StatusBadRequest)
        return
    }

    // Add timestamp if not provided
    if eventRequest.Data == nil {
        eventRequest.Data = make(map[string]interface{})
    }
    eventRequest.Data["timestamp"] = time.Now().Unix()
    eventRequest.Data["source"] = "go-api"

    err := s.client.TrackEvent(r.Context(), &earnly.Event{
        Name:   eventRequest.Event,
        UserID: eventRequest.UserID,
        Data:   eventRequest.Data,
    })

    response := map[string]interface{}{
        "success": err == nil,
    }

    if err != nil {
        response["error"] = err.Error()
        w.WriteHeader(http.StatusInternalServerError)
    }

    w.Header().Set("Content-Type", "application/json")
    json.NewEncoder(w).Encode(response)
}

func (s *Server) trackEventAsync(ctx context.Context, event *earnly.Event) {
    if err := s.client.TrackEvent(ctx, event); err != nil {
        log.Printf("Failed to track event: %v", err)
    }
}

func getClientIP(r *http.Request) string {
    // Check X-Forwarded-For header first
    forwarded := r.Header.Get("X-Forwarded-For")
    if forwarded != "" {
        return forwarded
    }
    
    // Check X-Real-IP header
    realIP := r.Header.Get("X-Real-IP")
    if realIP != "" {
        return realIP
    }
    
    // Fall back to RemoteAddr
    return r.RemoteAddr
}

func main() {
    server, err := NewServer("your-api-key")
    if err != nil {
        log.Fatal("Failed to create server:", err)
    }

    r := mux.NewRouter()
    
    // API routes
    api := r.PathPrefix("/api").Subrouter()
    api.HandleFunc("/recommendations/{userID}", server.GetRecommendations).Methods("GET")
    api.HandleFunc("/track", server.TrackEvent).Methods("POST")
    
    // Add CORS middleware
    r.Use(func(next http.Handler) http.Handler {
        return http.HandlerFunc(func(w http.ResponseWriter, r *http.Request) {
            w.Header().Set("Access-Control-Allow-Origin", "*")
            w.Header().Set("Access-Control-Allow-Methods", "GET, POST, OPTIONS")
            w.Header().Set("Access-Control-Allow-Headers", "Content-Type")
            
            if r.Method == "OPTIONS" {
                return
            }
            
            next.ServeHTTP(w, r)
        })
    })

    fmt.Println("Server starting on :8080")
    log.Fatal(http.ListenAndServe(":8080", r))
}</code></pre>
