&lt;?php

namespace App\Http\Controllers;

use Illuminate\Http\Request;
use Illuminate\Http\JsonResponse;
use Earnly\Laravel\EarnlyLaravelClient;
use Earnly\Model\RecommendationRequest as EarnlyRecommendationRequest;
use App\Http\Controllers\Controller;

class RecommendationController extends Controller
{
    private $earnlyClient;

    public function __construct(EarnlyLaravelClient $earnlyClient)
    {
        $this-&gt;earnlyClient = $earnlyClient;
    }

    /**
     * Get recommendations for a user
     */
    public function getRecommendations(Request $request, $userId): JsonResponse
    {
        $request-&gt;validate([
            'category' =&gt; 'sometimes|string',
            'page' =&gt; 'sometimes|string',
            'limit' =&gt; 'sometimes|integer|min:1|max:20'
        ]);

        try {
            $context = [
                'page' =&gt; $request-&gt;input('page', 'homepage'),
                'category' =&gt; $request-&gt;input('category'),
                'user_agent' =&gt; $request-&gt;userAgent(),
                'ip_address' =&gt; $request-&gt;ip(),
                'session_id' =&gt; $request-&gt;session()-&gt;getId(),
                'timestamp' =&gt; time()
            ];

            // Remove null values
            $context = array_filter($context);

            $recommendationRequest = new EarnlyRecommendationRequest([
                'user_id' =&gt; $userId,
                'context' =&gt; $context,
                'options' =&gt; [
                    'max_recommendations' =&gt; $request-&gt;input('limit', 6),
                    'personalized' =&gt; true,
                    'include_metadata' =&gt; true,
                    'filter_seen' =&gt; true
                ]
            ]);

            $recommendations = $this-&gt;earnlyClient-&gt;getRecommendations($recommendationRequest);

            // Track the recommendation request
            $this-&gt;earnlyClient-&gt;trackEventAsync('recommendations_viewed', $userId, [
                'context' =&gt; $context,
                'recommendation_count' =&gt; count($recommendations)
            ]);

            return response()-&gt;json([
                'success' =&gt; true,
                'data' =&gt; $recommendations,
                'count' =&gt; count($recommendations),
                'user_id' =&gt; $userId
            ]);

        } catch (\Exception $e) {
            \Log::error('Recommendation fetch failed', [
                'user_id' =&gt; $userId,
                'error' =&gt; $e-&gt;getMessage()
            ]);

            return response()-&gt;json([
                'success' =&gt; false,
                'error' =&gt; 'Failed to fetch recommendations'
            ], 500);
        }
    }

    /**
     * Track user events
     */
    public function trackEvent(Request $request): JsonResponse
    {
        $request-&gt;validate([
            'event' =&gt; 'required|string',
            'user_id' =&gt; 'required|string',
            'data' =&gt; 'sometimes|array'
        ]);

        try {
            $this-&gt;earnlyClient-&gt;trackEvent(
                $request-&gt;input('event'),
                $request-&gt;input('user_id'),
                array_merge(
                    $request-&gt;input('data', []),
                    [
                        'timestamp' =&gt; time(),
                        'source' =&gt; 'laravel-api',
                        'ip_address' =&gt; $request-&gt;ip()
                    ]
                )
            );

            return response()-&gt;json(['success' =&gt; true]);

        } catch (\Exception $e) {
            \Log::error('Event tracking failed', [
                'event' =&gt; $request-&gt;input('event'),
                'user_id' =&gt; $request-&gt;input('user_id'),
                'error' =&gt; $e-&gt;getMessage()
            ]);

            return response()-&gt;json([
                'success' =&gt; false,
                'error' =&gt; 'Failed to track event'
            ], 500);
        }
    }
}

// routes/api.php
Route::middleware('api')-&gt;group(function () {
    Route::get('recommendations/{userId}', [RecommendationController::class, 'getRecommendations']);
    Route::post('track', [RecommendationController::class, 'trackEvent']);
});</code></pre>
