/**
 * Plugin Name: Earnly Recommendations
 * Description: AI-powered content and product recommendations for WordPress
 * Version: 1.0.0
 * Author: Earnly
 */

// Prevent direct access
if (!defined('ABSPATH')) {
    exit;
}

require_once plugin_dir_path(__FILE__) . 'vendor/autoload.php';

use Earnly\Client\EarnlyClient;
use Earnly\Configuration\Configuration;
use Earnly\Model\RecommendationRequest;

class EarnlyWordPressPlugin
{
    private $client;
    
    public function __construct()
    {
        add_action('init', [$this, 'init']);
        add_action('wp_enqueue_scripts', [$this, 'enqueue_scripts']);
        add_shortcode('earnly_recommendations', [$this, 'recommendations_shortcode']);
        add_action('wp_ajax_earnly_track', [$this, 'handle_track_event']);
        add_action('wp_ajax_nopriv_earnly_track', [$this, 'handle_track_event']);
    }
    
    public function init()
    {
        $config = new Configuration([
            'api_key' =&gt; get_option('earnly_api_key'),
            'environment' =&gt; get_option('earnly_environment', 'production'),
            'timeout' =&gt; 5.0,
            'cache_enabled' =&gt; true,
            'cache_ttl' =&gt; 300
        ]);
        
        $this-&gt;client = new EarnlyClient($config);
    }
    
    public function enqueue_scripts()
    {
        wp_enqueue_script(
            'earnly-recommendations',
            plugin_dir_url(__FILE__) . 'assets/earnly-recommendations.js',
            ['jquery'],
            '1.0.0',
            true
        );
        
        wp_localize_script('earnly-recommendations', 'earnly_ajax', [
            'ajax_url' =&gt; admin_url('admin-ajax.php'),
            'nonce' =&gt; wp_create_nonce('earnly_nonce')
        ]);
    }
    
    /**
     * Shortcode: [earnly_recommendations user_id="123" category="products" limit="6"]
     */
    public function recommendations_shortcode($atts)
    {
        $atts = shortcode_atts([
            'user_id' =&gt; get_current_user_id(),
            'category' =&gt; '',
            'limit' =&gt; 6,
            'layout' =&gt; 'grid',
            'show_images' =&gt; true
        ], $atts);
        
        if (empty($atts['user_id'])) {
            return '&lt;p&gt;User ID required for recommendations&lt;/p&gt;';
        }
        
        try {
            $context = [
                'page' =&gt; is_home() ? 'homepage' : get_post_type(),
                'category' =&gt; $atts['category'],
                'post_id' =&gt; get_the_ID(),
                'user_agent' =&gt; $_SERVER['HTTP_USER_AGENT'] ?? '',
                'timestamp' =&gt; time()
            ];
            
            $request = new RecommendationRequest([
                'user_id' =&gt; $atts['user_id'],
                'context' =&gt; array_filter($context),
                'options' =&gt; [
                    'max_recommendations' =&gt; intval($atts['limit']),
                    'personalized' =&gt; true,
                    'include_metadata' =&gt; true
                ]
            ]);
            
            $recommendations = $this-&gt;client-&gt;getRecommendations($request);
            
            return $this-&gt;render_recommendations($recommendations, $atts);
            
        } catch (Exception $e) {
            error_log('Earnly WordPress error: ' . $e-&gt;getMessage());
            return '&lt;p&gt;Unable to load recommendations&lt;/p&gt;';
        }
    }
    
    private function render_recommendations($recommendations, $atts)
    {
        if (empty($recommendations)) {
            return '&lt;p&gt;No recommendations available&lt;/p&gt;';
        }
        
        $html = '&lt;div class="earnly-recommendations layout-' . esc_attr($atts['layout']) . '"&gt;';
        $html .= '&lt;h3&gt;Recommended for You&lt;/h3&gt;';
        $html .= '&lt;div class="recommendations-grid"&gt;';
        
        foreach ($recommendations as $rec) {
            $html .= '&lt;div class="recommendation-item" data-id="' . esc_attr($rec['id']) . '"&gt;';
            
            if ($atts['show_images'] && !empty($rec['image_url'])) {
                $html .= '&lt;img src="' . esc_url($rec['image_url']) . '" alt="' . esc_attr($rec['title']) . '" /&gt;';
            }
            
            $html .= '&lt;h4&gt;&lt;a href="' . esc_url($rec['click_url']) . '" class="recommendation-link"&gt;';
            $html .= esc_html($rec['title']) . '&lt;/a&gt;&lt;/h4&gt;';
            $html .= '&lt;p&gt;' . esc_html($rec['description']) . '&lt;/p&gt;';
            $html .= '&lt;/div&gt;';
        }
        
        $html .= '&lt;/div&gt;&lt;/div&gt;';
        
        return $html;
    }
    
    public function handle_track_event()
    {
        if (!wp_verify_nonce($_POST['nonce'], 'earnly_nonce')) {
            wp_die('Security check failed');
        }
        
        $event = sanitize_text_field($_POST['event']);
        $userId = sanitize_text_field($_POST['user_id']);
        $data = json_decode(stripslashes($_POST['data']), true);
        
        try {
            $this-&gt;client-&gt;trackEvent($event, $userId, $data);
            wp_send_json_success();
        } catch (Exception $e) {
            wp_send_json_error($e-&gt;getMessage());
        }
    }
}

// Initialize the plugin
new EarnlyWordPressPlugin();</code></pre>
