INSTALLED_APPS = [
    # ... other apps
    'earnly.django',
]

EARNLY_CONFIG = {
    'API_KEY': os.environ.get('EARNLY_API_KEY'),
    'ENVIRONMENT': 'production',
    'AUTO_TRACK_VIEWS': True,
    'AUTO_TRACK_USERS': True,
    'MIDDLEWARE_ENABLED': True,
    'CACHE_BACKEND': 'redis',
    'CACHE_TTL': 300
}

MIDDLEWARE = [
    # ... other middleware
    'earnly.django.middleware.EarnlyMiddleware',
]

# views.py
from django.http import JsonResponse
from django.views.decorators.http import require_http_methods
from earnly.django import get_earnly_client, track_event
from django.contrib.auth.decorators import login_required
import json

@require_http_methods(["GET"])
@login_required
def recommendations_view(request):
    """Django view for serving recommendations"""
    client = get_earnly_client()
    user_id = str(request.user.id)
    
    # Extract context from request
    context = {
        'page': request.GET.get('page', 'homepage'),
        'category': request.GET.get('category'),
        'user_agent': request.META.get('HTTP_USER_AGENT'),
        'ip_address': get_client_ip(request),
        'referrer': request.META.get('HTTP_REFERER'),
        'session_key': request.session.session_key
    }
    
    try:
        recommendations = client.get_recommendations(
            user_id=user_id,
            context=context,
            options={
                'max_recommendations': int(request.GET.get('limit', 6)),
                'personalized': True,
                'include_metadata': True
            }
        )
        
        # Automatically track the recommendation request
        track_event(
            event='recommendations_viewed',
            user_id=user_id,
            data={
                'context': context,
                'recommendation_count': len(recommendations)
            }
        )
        
        return JsonResponse({
            'success': True,
            'recommendations': recommendations,
            'count': len(recommendations)
        })
        
    except Exception as e:
        return JsonResponse({
            'success': False,
            'error': str(e)
        }, status=500)

def get_client_ip(request):
    """Extract client IP address from request"""
    x_forwarded_for = request.META.get('HTTP_X_FORWARDED_FOR')
    if x_forwarded_for:
        ip = x_forwarded_for.split(',')[0]
    else:
        ip = request.META.get('REMOTE_ADDR')
    return ip

# models.py - Optional Django model integration
