                                    <pre class="bg-gray-900 text-gray-100 p-3 rounded text-sm overflow-x-auto"><code class="language-go">// Batch process multiple users concurrently
type UserRequest struct {
    UserID  string
    Context map[string]interface{}
}

func (s *Server) BatchRecommendations(
    ctx context.Context, 
    requests []UserRequest
) ([][]earnly.Recommendation, error) {
    results := make([][]earnly.Recommendation, len(requests))
    errChan := make(chan error, len(requests))
    
    var wg sync.WaitGroup
    for i, req := range requests {
        wg.Add(1)
        go func(index int, userReq UserRequest) {
            defer wg.Done()
            
            recs, err := s.client.GetRecommendations(ctx, &earnly.RecommendationRequest{
                UserID:  userReq.UserID,
                Context: userReq.Context,
                Options: &earnly.RecommendationOptions{
                    MaxRecommendations: 6,
                    Personalized:      true,
                },
            })
            
            if err != nil {
                errChan <- err
                return
            }
            
            results[index] = recs
        }(i, req)
    }
    
    wg.Wait()
    close(errChan)
    
    // Check for errors
    if err := <-errChan; err != nil {
        return nil, err
    }
    
    return results, nil
}</code></pre>
