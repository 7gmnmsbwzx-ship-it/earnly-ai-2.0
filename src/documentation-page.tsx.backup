                            <pre class="bg-gray-900 text-gray-100 p-4 rounded-lg overflow-x-auto"><code class="language-javascript">const express = require('express')
const { EarnlyClient, EarnlyMiddleware } = require('@earnly/node-sdk')

const app = express()
const earnly = new EarnlyClient({
  apiKey: process.env.EARNLY_API_KEY,
  environment: process.env.NODE_ENV === 'production' ? 'production' : 'sandbox'
})

// Add Earnly middleware for automatic tracking
app.use(EarnlyMiddleware({
  client: earnly,
  autoTrack: {
    pageViews: true,
    userSessions: true,
    apiCalls: false
  },
  excludePaths: ['/health', '/metrics', '/admin']
}))

// Recommendations endpoint
app.get('/api/recommendations/:userId', async (req, res) =&gt; {
  try {
    const { userId } = req.params
    const { category, page, limit = 6 } = req.query
    
    // Extract context from request
    const context = {
      userId,
      page: page || 'homepage',
      category,
      userAgent: req.get('User-Agent'),
      ipAddress: req.ip,
      referrer: req.get('Referer'),
      sessionId: req.sessionID,
      timestamp: Date.now()
    }
    
    // Get personalized recommendations
    const recommendations = await earnly.getRecommendations({
      userId,
      context,
      options: {
        maxRecommendations: parseInt(limit),
        includeMetadata: true,
        personalized: true,
        filterSeen: true // Exclude previously seen recommendations
      }
    })
    
    // Track recommendation request
    await earnly.trackEvent('recommendations_requested', {
      userId,
      context,
      recommendationCount: recommendations.length
    })
    
    res.json({
      success: true,
      data: recommendations,
      metadata: {
        userId,
        timestamp: Date.now(),
        count: recommendations.length
      }
    })
    
  } catch (error) {
    console.error('Recommendation API error:', error)
    res.status(500).json({
      success: false,
      error: error.message
    })
  }
})

// Event tracking endpoint
app.post('/api/track', async (req, res) =&gt; {
  try {
    const { event, userId, data } = req.body
    
    if (!event || !userId) {
      return res.status(400).json({
        success: false,
        error: 'Event and userId are required'
      })
    }
    
    await earnly.trackEvent(event, {
      userId,
      ...data,
      timestamp: Date.now(),
      source: 'api'
    })
    
    res.json({ success: true })
    
  } catch (error) {
    console.error('Event tracking error:', error)
    res.status(500).json({
      success: false,
      error: error.message
    })
  }
})

// User profile update endpoint
app.put('/api/users/:userId/profile', async (req, res) =&gt; {
  try {
    const { userId } = req.params
    const profileData = req.body
    
    await earnly.updateUserProfile(userId, {
      ...profileData,
      lastUpdated: Date.now()
    })
    
    res.json({ success: true })
    
  } catch (error) {
    console.error('Profile update error:', error)
    res.status(500).json({
      success: false,
      error: error.message
    })
  }
})

const PORT = process.env.PORT || 3000
app.listen(PORT, () =&gt; {
  console.log(\`Server running on port \${PORT}\`)
})</code></pre>
