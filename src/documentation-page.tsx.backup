// Initialize the client
$config = new Configuration([
    'api_key' =&gt; 'your-api-key',
    'environment' =&gt; 'production', // or 'sandbox'
    'timeout' =&gt; 5.0,
    'retry_attempts' =&gt; 3,
    'enable_logging' =&gt; true,
    'cache_enabled' =&gt; true,
    'cache_ttl' =&gt; 300
]);

$client = new EarnlyClient($config);

/**
 * Get personalized recommendations for a user
 */
function getRecommendations($client, $userId, $context = []) {
    try {
        $request = new RecommendationRequest([
            'user_id' =&gt; $userId,
            'context' =&gt; array_merge([
                'page' =&gt; 'homepage',
                'timestamp' =&gt; time()
            ], $context),
            'options' =&gt; [
                'max_recommendations' =&gt; 6,
                'personalized' =&gt; true,
                'include_metadata' =&gt; true,
                'filter_seen' =&gt; true
            ]
        ]);
        
        $recommendations = $client-&gt;getRecommendations($request);
        return $recommendations;
        
    } catch (Exception $e) {
        error_log('Failed to fetch recommendations: ' . $e-&gt;getMessage());
        return [];
    }
}

/**
 * Track user events
 */
function trackEvent($client, $eventName, $userId, $data = []) {
    try {
        $event = new Event([
            'name' =&gt; $eventName,
            'user_id' =&gt; $userId,
            'data' =&gt; array_merge($data, [
                'timestamp' =&gt; time(),
                'source' =&gt; 'php-sdk'
            ])
        ]);
        
        $client-&gt;trackEvent($event);
        return true;
        
    } catch (Exception $e) {
        error_log('Failed to track event: ' . $e-&gt;getMessage());
        return false;
    }
}

