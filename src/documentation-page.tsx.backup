  filters?: Record&lt;string, any&gt;
}

@Injectable({
  providedIn: 'root'
})
export class RecommendationService {
  private contextSubject = new BehaviorSubject&lt;RecommendationContext&gt;({
    userId: '',
    page: 'home'
  })

  public context$ = this.contextSubject.asObservable()

  constructor(private earnly: EarnlyService) {}

  getRecommendations(context?: RecommendationContext): Observable&lt;Recommendation[]&gt; {
    if (context) {
      this.updateContext(context)
    }
    return this.earnly.getRecommendations(this.contextSubject.value)
  }

  updateContext(context: Partial&lt;RecommendationContext&gt;): void {
    const currentContext = this.contextSubject.value
    this.contextSubject.next({ ...currentContext, ...context })
  }

  trackInteraction(event: string, data: any): void {
    this.earnly.trackEvent(event, {
      ...data,
      context: this.contextSubject.value,
      timestamp: Date.now()
    })
  }

  updateUserProfile(profile: Record&lt;string, any&gt;): Observable&lt;any&gt; {
    return this.earnly.updateUserProfile(profile)
  }
}</code></pre>
