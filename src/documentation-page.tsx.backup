                            <pre class="bg-gray-900 text-gray-100 p-4 rounded-lg overflow-x-auto"><code class="language-python">from fastapi import FastAPI, Depends, HTTPException, Request
from fastapi.middleware.cors import CORSMiddleware
from earnly.fastapi import EarnlyMiddleware, EarnlyClient, get_earnly_client
from pydantic import BaseModel
from typing import List, Optional, Dict, Any
import asyncio

app = FastAPI(title="Recommendation API", version="1.0.0")

# Add Earnly middleware
app.add_middleware(
    EarnlyMiddleware,
    api_key="your-api-key",
    auto_track_requests=True,
    exclude_paths=["/health", "/docs", "/openapi.json"]
)

app.add_middleware(
    CORSMiddleware,
    allow_origins=["*"],
    allow_methods=["*"],
    allow_headers=["*"]
)

# Pydantic models
class RecommendationRequest(BaseModel):
    user_id: str
    context: Dict[str, Any] = {}
    max_recommendations: int = 6
    personalized: bool = True

class EventTrackingRequest(BaseModel):
    event: str
    user_id: str
    data: Dict[str, Any] = {}

class RecommendationResponse(BaseModel):
    id: str
    title: str
    description: str
    image_url: str
    click_url: str
    metadata: Dict[str, Any]

@app.get("/health")
async def health_check():
    return {"status": "healthy", "service": "earnly-recommendations"}

@app.post("/recommendations", response_model=List[RecommendationResponse])
async def get_recommendations(
    request: RecommendationRequest,
    http_request: Request,
    client: EarnlyClient = Depends(get_earnly_client)
):
    """Get personalized recommendations"""
    try:
        # Enhance context with request information
        enhanced_context = {
            **request.context,
            "user_agent": http_request.headers.get("user-agent"),
            "ip_address": http_request.client.host,
            "timestamp": int(asyncio.get_event_loop().time())
        }
        
        recommendations = await client.get_recommendations_async(
            user_id=request.user_id,
            context=enhanced_context,
            options={
                "max_recommendations": request.max_recommendations,
                "personalized": request.personalized,
                "include_metadata": True
            }
        )
        
        return recommendations
        
    except Exception as e:
        raise HTTPException(status_code=500, detail=str(e))

@app.post("/track")
async def track_event(
    request: EventTrackingRequest,
    client: EarnlyClient = Depends(get_earnly_client)
):
    """Track user events"""
    try:
        await client.track_event_async(
            event=request.event,
            user_id=request.user_id,
            data={
                **request.data,
                "timestamp": int(asyncio.get_event_loop().time())
            }
        )
        
        return {"success": True}
        
    except Exception as e:
        raise HTTPException(status_code=500, detail=str(e))

@app.get("/recommendations/{user_id}")
async def get_user_recommendations(
    user_id: str,
    category: Optional[str] = None,
    page: str = "homepage",
    limit: int = 6,
    client: EarnlyClient = Depends(get_earnly_client)
):
    """Get recommendations for a specific user"""
    context = {
        "page": page,
        "category": category
    }
    
    recommendations = await client.get_recommendations_async(
        user_id=user_id,
        context=context,
        options={"max_recommendations": limit}
    )
    
    return {
        "user_id": user_id,
        "recommendations": recommendations,
        "count": len(recommendations)
    }

if __name__ == "__main__":
    import uvicorn
    uvicorn.run(app, host="0.0.0.0", port=8000)</code></pre>
