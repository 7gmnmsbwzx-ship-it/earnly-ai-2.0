                            <pre class="bg-gray-900 text-gray-100 p-4 rounded-lg overflow-x-auto"><code class="language-python">from earnly import EarnlyClient
import asyncio
from typing import List, Dict, Optional

# Initialize client
client = EarnlyClient(
    api_key="your-api-key",
    environment="production",  # or "sandbox"
    config={
        "timeout": 5.0,
        "retry_attempts": 3,
        "enable_logging": True,
        "cache_enabled": True,
        "cache_ttl": 300
    }
)

# Synchronous recommendations
def get_recommendations(user_id: str, context: Dict) -&gt; List[Dict]:
    """Get personalized recommendations for a user"""
    try:
        recommendations = client.get_recommendations(
            user_id=user_id,
            context={
                "page": "homepage",
                "category": "electronics",
                "user_agent": context.get("user_agent"),
                "ip_address": context.get("ip_address"),
                **context
            },
            options={
                "max_recommendations": 6,
                "personalized": True,
                "include_metadata": True,
                "filter_seen": True
            }
        )
        return recommendations
    except Exception as e:
        print(f"Error fetching recommendations: {e}")
        return []

# Asynchronous recommendations
async def get_recommendations_async(user_id: str, context: Dict) -&gt; List[Dict]:
    """Async version for high-performance applications"""
    async with client.async_session() as session:
        recommendations = await session.get_recommendations(
            user_id=user_id,
            context=context,
            options={"max_recommendations": 6}
        )
        return recommendations

# Event tracking
def track_user_event(user_id: str, event: str, data: Dict) -&gt; bool:
    """Track user events and interactions"""
    try:
        client.track_event(
            event=event,
            user_id=user_id,
            data={
                **data,
                "timestamp": int(time.time()),
                "source": "python-sdk"
            }
        )
        return True
    except Exception as e:
        print(f"Error tracking event: {e}")
        return False

# Example usage
if __name__ == "__main__":
    context = {
        "user_agent": "Mozilla/5.0...",
        "ip_address": "192.168.1.1",
        "referrer": "https://google.com"
    }
    
    # Get recommendations
    recs = get_recommendations("user-123", context)
    print(f"Found {len(recs)} recommendations")
    
    # Track page view
    track_user_event("user-123", "page_view", {
        "page": "homepage", 
        "category": "electronics"
    })</code></pre>
