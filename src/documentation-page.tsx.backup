import { ref, reactive, computed, watch } from 'vue'
import { useEarnly } from '@earnly/vue-sdk'

// Initialize Earnly composable
const { 
  recommendations, 
  isLoading, 
  error, 
  trackEvent, 
  updateContext,
  refreshRecommendations 
} = useEarnly()

// Reactive context
const context = reactive({
  userId: 'user-123',
  page: 'homepage',
  category: 'featured',
  filters: {
    priceRange: [0, 1000],
    inStock: true
  }
})

// Update recommendations when context changes
watch(context, (newContext) =&gt; {
  updateContext(newContext)
}, { deep: true })

// Computed properties
const filteredRecommendations = computed(() =&gt; {
  return recommendations.value.filter(rec =&gt; 
    rec.metadata.price &lt;= context.filters.priceRange[1]
  )
})

// Event handlers
const handleInteraction = (type: string, data: any) =&gt; {
  trackEvent(type, {
    ...data,
    context: context,
    timestamp: Date.now()
  })
}

// Manual recommendation fetching
const loadMoreRecommendations = async () =&gt; {
  try {
    await refreshRecommendations({
      ...context,
      limit: 12,
      offset: recommendations.value.length
    })
  } catch (err) {
    console.error('Failed to load more recommendations:', err)
  }
}
&lt;/script&gt;</code></pre>
