import { Component, OnInit, OnDestroy, Input, Output, EventEmitter, ChangeDetectionStrategy } from '@angular/core'
import { Observable, Subject, combineLatest } from 'rxjs'
import { takeUntil, switchMap, startWith, catchError } from 'rxjs/operators'
import { RecommendationService, Recommendation, RecommendationContext } from './recommendation.service'

@Component({
  selector: 'app-recommendations',
  templateUrl: './recommendations.component.html',
  styleUrls: ['./recommendations.component.scss'],
  changeDetection: ChangeDetectionStrategy.OnPush
})
export class RecommendationsComponent implements OnInit, OnDestroy {
  @Input() context: RecommendationContext = { userId: '', page: 'default' }
  @Input() maxRecommendations: number = 6
  @Input() layout: 'grid' | 'list' | 'carousel' = 'grid'
  @Input() enableTracking: boolean = true
  
  @Output() recommendationClick = new EventEmitter&lt;Recommendation&gt;()
  @Output() recommendationsLoaded = new EventEmitter&lt;Recommendation[]&gt;()
  @Output() error = new EventEmitter&lt;Error&gt;()

  recommendations$: Observable&lt;Recommendation[]&gt;
  isLoading = true
  hasError = false
  
  private destroy$ = new Subject&lt;void&gt;()
  private refresh$ = new Subject&lt;void&gt;()

  constructor(
    private recommendationService: RecommendationService
  ) {
    this.recommendations$ = combineLatest([
      this.recommendationService.context$,
      this.refresh$.pipe(startWith(null))
    ]).pipe(
      switchMap(() =&gt; this.recommendationService.getRecommendations()),
      catchError((error) =&gt; {
        this.hasError = true
        this.error.emit(error)
        return []
      }),
      takeUntil(this.destroy$)
    )
  }

  ngOnInit(): void {
    if (this.context) {
      this.recommendationService.updateContext(this.context)
    }

    this.recommendations$.subscribe({
      next: (recommendations) =&gt; {
        this.isLoading = false
        this.hasError = false
        this.recommendationsLoaded.emit(recommendations.slice(0, this.maxRecommendations))
      },
      error: (error) =&gt; {
        this.isLoading = false
        this.hasError = true
        this.error.emit(error)
      }
    })

    // Track component view
    if (this.enableTracking) {
      this.recommendationService.trackInteraction('recommendations_view', {
        component: 'RecommendationsComponent',
        layout: this.layout,
        maxRecommendations: this.maxRecommendations
      })
    }
  }

  ngOnDestroy(): void {
    this.destroy$.next()
    this.destroy$.complete()
  }

  onRecommendationClick(recommendation: Recommendation): void {
    if (this.enableTracking) {
      this.recommendationService.trackInteraction('recommendation_click', {
        recommendationId: recommendation.id,
        title: recommendation.title,
        category: recommendation.category,
        position: 0 // You might want to track position
      })
    }
    
    this.recommendationClick.emit(recommendation)
  }

  refresh(): void {
    this.isLoading = true
    this.hasError = false
    this.refresh$.next()
  }

  trackByRecommendation(index: number, recommendation: Recommendation): string {
    return recommendation.id
  }
}</code></pre>
